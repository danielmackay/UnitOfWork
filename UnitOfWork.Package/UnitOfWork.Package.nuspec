<?xml version="1.0"?>
<package >
  <metadata>
    <id>UnitOfWork</id>
    <version>0.13</version>
    <authors>Daniel Mackay</authors>
    <owners>Daniel Mackay</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      The UnitOfWork package provides a lightweight starter kit for using the UOW, and Repository patterns with Entity Framework.  It has been implemented in an easily testable way, so that you can mock data access to make testing easier.
    </description>
    <releaseNotes></releaseNotes>
    <copyright>Copyright 2013</copyright>
    <tags>UOW, Unit Of Work, EF, Entity Framework, Repository, Code First, IoC, Mocking, Unit Test</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.0">
        <dependency id="EntityFramework" version="5.0.0" />
        <dependency id="Unity" version="2.1.505.2" />
      </group>
    </dependencies>
  </metadata>
</package>